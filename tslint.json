{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "no-unused-variable": true,
    "curly": true,
    "class-name": true,
    "semicolon": true,
    "single-quote": true,
    "triple-equals": true,
    "quotemark": [true, "single"],
    "no-console": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
